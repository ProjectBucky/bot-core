  <category>
    <pattern>WHAT IS MY FAVE CHEESE *</pattern>
    <template>
      <condition name="favcheese">
        <li value="undefined">
          <random>
            <li>You haven't told me your fave cheese.</li>
            <li>I won't know until you tell me.</li>
          </random>
        </li>
        <li value="SEEKER">
          <random>
            <li>You haven't told me your fave cheese.</li>
            <li>I won't know until you tell me.</li>
          </random>
        </li>
        <li>Your fave cheese is <get name="favcheese"/>.</li>
      </condition>
    </template>
  </category>
